{
  "name": "ffpi/nodecounter",
  "description": "Displays a Freifunk Nodecounter",
  "homepage": "https://pinneberg.freifunk.net",
  "license": "GPL-3.0-or-later",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "Kevin Quiatkowski",
      "role": "Developer"
    }
  ],
  "support": {
    "forum": "https://pinneberg.freifunk.net/forum/",
    "source": "https://github.com/freifunk-pinneberg/typo3-tx_ffpi_nodecounter",
    "issues": "https://github.com/freifunk-pinneberg/typo3-tx_ffpi_nodecounter/issues"
  },
  "funding": [
    {
      "type": "other",
      "url": "https://pinneberg.freifunk.net/mitmachen/spenden"
    }
  ],
  "keywords": [
    "Freifunk",
    "Pinneberg",
    "nodes",
    "counter",
    "ffmap-backend"
  ],
  "replace": {
    "typo3-ter/ffpi-nodecounter": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "web-dir": ".Build/web"
  },
  "minimum-stability": "beta",
  "require": {
    "typo3/cms-core": "10.0.0 - 11.5.99"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "FFPI\\FfpiNodecounter\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "post-update-cmd": [
    ],
    "post-install-cmd": [
    ],
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/ffpi_nodecounter ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/ffpi_nodecounter"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "ffpi_nodecounter"
    }
  }
}
